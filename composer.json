{
  "name": "jield-webdev/assetic",
  "description": "Asset Management for PHP",
  "keywords": [
    "assets",
    "compression",
    "minification"
  ],
  "homepage": "https://github.com/jield-webdev/assetic",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Kris Wallsmith",
      "email": "kris.wallsmith@gmail.com",
      "homepage": "http://kriswallsmith.net/"
    },
    {
      "name": "Jack Wilkinson",
      "email": "me@jackwilky.com",
      "homepage": "https://jackwilky.com/"
    },
    {
      "name": "Luke Towers",
      "email": "octobercms@luketowers.ca",
      "homepage": "https://luketowers.ca"
    },
    {
      "name": "Johan van der Heide",
      "email": "info@jield.nl",
      "homepage": "https://jield.nl"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-simplexml": "*",
    "symfony/process": "~4.0",
    "twig/twig": "3.*"
  },
  "conflict": {
    "twig/twig": "<1.27"
  },
  "require-dev": {
    "wikimedia/less.php": "dev-master",
    "scssphp/scssphp": "~1.0",
    "meenie/javascript-packer": "^1.1",
    "mrclay/minify": "<2.3",
    "natxet/cssmin": "3.0.6",
    "patchwork/jsqueeze": "~1.0|~2.0",
    "phpunit/phpunit": "~8.0",
    "psr/log": "~1.0",
    "ptachoire/cssembed": "~1.0",
    "symfony/phpunit-bridge": "~2.7|~3.0",
    "twig/twig": "~2.11"
  },
  "suggest": {
    "wikimedia/less.php": "The Assetic\\Filter\\LessphpFilter requires wikimedia/less.php",
    "scssphp/scssphp": "The Assetic\\Filter\\ScssphpFilter requires scssphp/scssphp",
    "meenie/javascript-packer": "The Assetic\\Filter\\PackerFilter requires meenie/javascript-packer",
    "mrclay/minify": "The Assetic\\Filter\\JSMinFilter requires mrclay/minify",
    "natxet/cssmin": "The Assetic\\Filter\\CssMinFilter requires natxet/cssmin",
    "patchwork/jsqueeze": "The Assetic\\Filter\\JSqueezeFilter requires patchwork/jsqueeze",
    "ptachoire/cssembed": "The Assetic\\Filter\\PhpCssEmbedFilter requires ptachoire/cssembed",
    "twig/twig": "Assetic provides an integration with the Twig templating engine"
  },
  "replace": {
    "kriswallsmith/assetic": "*"
  },
  "autoload": {
    "psr-0": {
      "Assetic": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev",
      "dev-develop": "1.1-dev"
    }
  }
}
